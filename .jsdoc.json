{
  "opts": {
    "destination": "public",
    "readme": "README.md",
    "template": "docs/minami"
  },
  "markdown": {
    "hardWrap": true
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": ["src/config.js",
        "src/evaluator.js",  
        "src/interceptor.js", 
        "src/job.js", 
        "src/metrics.js",
        "src/printer.js", 
        "src/source.js"]
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "collapse": true,
      "disableSort": false,
      "monospaceLinks": false,
      "referenceTitle": "Bespoken Batch Tester"
  }
}