{
  "opts": {
    "destination": "../../docs/api/batch",
    "readme": "README.md",
    "template": "docs/minami"
  },
  "markdown": {
    "hardWrap": true
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": [
        "lib/device.js",
        "lib/evaluator.js",  
        "lib/interceptor.js", 
        "lib/job.js", 
        "lib/metrics.js",
        "lib/printer.js", 
        "lib/record.js", 
        "lib/result.js", 
        "lib/source.js"]
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "collapse": true,
      "disableSort": false,
      "monospaceLinks": false,
      "referenceTitle": "Bespoken Batch SDK"
  }
}
