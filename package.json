{
  "author": "Bespoken",
  "description": "This project tests utterances in batch - supporting a variety of data sources and reporting formats",
  "devDependencies": {
    "@types/node": "^16.9.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-jsdoc": "^36.1.0",
    "jest": "^24.9.0",
    "jest-junit-reporter": "^1.1.0",
    "jsdoc": "^3.6.5",
    "lerna": "^4.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "index.js",
    "bin/*.js",
    "src/*.js"
  ],
  "homepage": "https://gitlab.com/bespoken/batch-tester.git",
  "license": "ISC",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ],
    "testResultsProcessor": "./node_modules/jest-junit-reporter"
  },
  "main": "index.js",
  "name": "batch-tester",
  "scripts": {
    "codecov": "codecov",
    "compile": "lerna exec -- tsc --outDir types",
    "docs": "jsdoc -c .jsdoc.json",
    "postversion": "git push && git push --tags",
    "process": "node bin/bbt.js process",
    "standard": "standard --fix",
    "test": "jest"
  },
  "standard": {}
}
